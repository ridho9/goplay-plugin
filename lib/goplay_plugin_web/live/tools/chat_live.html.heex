<div class="mb-2">
  <h1 class="text-xl">Chat Tools</h1>
</div>
<.form let={f} for={:input} phx-submit="run">
  <div class="mb-1">
    <%= label f, :event_slug, "Event Slug", class: "block" %>
    <%= text_input f, :event_slug, class: "block border rounded w-1/2", value: "playing-funn" %>
  </div>
  <div class="mt-2">
    <%= submit "Run", class: "border rounded p-1" %>
  </div>
</.form>
